package polymorghism;


// 결합도(Coipling)란 하나의 클래스가 다른 클래스와 얼마나 많이 연결되어 있는지를 나타내는 표현이며,
// 결합도(coupling)가 높은 프로그램은 유지보사가 비교적 어렵습니다.
public class TVUser {

	public static void main(String[] args) {

		SamsungTV jangnaratv = new SamsungTV();
		jangnaratv.PowerOn();
		jangnaratv.volumUp();
		jangnaratv.volumDown();
		jangnaratv.PowerOff();
		
		
	}

}
